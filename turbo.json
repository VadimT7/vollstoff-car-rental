{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {},
    "typecheck": {},
    "test": {},
    "test:unit": {},
    "test:e2e": {},
    "clean": {
      "cache": false
    },
    "start": {
      "dependsOn": ["build"]
    },
    "deploy": {
      "dependsOn": ["build"]
    }
  }
}
